#include "../interface/TGenLepton.h"

GenLepton::GenLepton(){
}

GenLepton::GenLepton(double pttemp, double etatemp, double phitemp, int chargetemp, int statustemp, int idtemp, int motherIdtemp) :
  pt(pttemp), eta(etatemp), phi(phitemp), charge(chargetemp), status(statustemp), id(idtemp), motherId(motherIdtemp)
{
  setLV();
}
